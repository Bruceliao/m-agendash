{
  "name": "m-agendash",
  "version": "0.1.0",
  "description": "Agenda Dashboard",
  "main": "app.js",
  "bin": "bin/agendash-standalone.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "test": "run-p lint ava",
    "lint": "xo",
    "ava": "ava"
  },
  "keywords": [
    "agenda"
  ],
  "repository": {
    "type": "git"
  },
  "license": "MIT",
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.15.0",
    "commander": "^2.9.0",
    "cron-parser": "^2.4.4",
    "express": "^4.0.0",
    "layui-laydate": "^5.0.85",
    "m-agenda": "git+https://github.com/Bruceliao/m-agenda.git",
    "semver": "^5.3.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "npm-run-all": "^4.1.2",
    "supertest": "^3.0.0",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": 2,
    "rules": {
      "space-before-function-paren": [
        "error",
        "never"
      ],
      "max-params": [
        "error",
        5
      ],
      "max-nested-callbacks": [
        "error",
        5
      ]
    },
    "envs": [
      "node"
    ],
    "ignores": [
      "public/**/*"
    ]
  }
}
